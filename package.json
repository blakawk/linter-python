{
  "name": "linter-python",
  "main": "./lib/init",
  "version": "3.0.0",
  "description": "Linter for python files. Lint operation based on pylama application.",
  "repository": "https://github.com/pchomik/linter-python",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
      "start": "npm run tsc:w",
      "postinstall": "typings install",
      "tsc": "tsc",
      "tsc:w": "tsc -w",
      "typings": "typings"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-package-deps": "^4.0.1",
    "child-process-es6-promise": "^1.0.0",
    "temp": "^0.8.3"
  },
  "devDependencies": {
      "typescript": "^1.8.10",
      "typings":"^1.0.4",
      "atom":"^1.0.0"
  },
  "package-deps": [
    "linter"
  ]
}
